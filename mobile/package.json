{
  "name": "skatehubba-mobile",
  "version": "0.0.1",
  "private": true,
  "description": "SkateHubba mobile app (Expo) with enterprise delivery.",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:smoke": "detox test --configuration ios.sim.debug --testNamePattern \"Smoke\"",
    "e2e:challenge": "detox test --configuration ios.sim.debug --testNamePattern \"Challenge\""
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "detox": "^20.30.0",
    "jest": "^29.7.0",
    "jest-expo": "^50.0.1"
  }
}
